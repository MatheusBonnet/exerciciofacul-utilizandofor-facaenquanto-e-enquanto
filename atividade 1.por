programa {
	funcao inicio() {
		
		escreva("PARA", "\n")
		para(inteiro n = 1; n < 50; n++){
		    escreva(n, "\n")
		}
		escreva("ENQUANTO","\n")
		inteiro n2 = 0
		enquanto(n2 < 50){
		   escreva(n2, "\n")
		    n2++
		}
		
		escreva("FACA ENQUANTO","\n")
		inteiro n3 = 0
		faca{
		    escreva(n3, "\n")
		    n3++
		}
	    enquanto(n3 <= 50)
	}
}
