programa {
	funcao inicio() {
		
		escreva("PARA", "\n")
		para(inteiro n = 100; n < 201; n++){
		    escreva(n, "\n")
		}
		escreva("ENQUANTO","\n")
		inteiro n2 = 100
		enquanto(n2 < 201){
		   escreva(n2, "\n")
		    n2++
		}
		
		escreva("FACA ENQUANTO","\n")
		inteiro n3 = 100
		faca{
		    escreva(n3, "\n")
		    n3++
		}
	    enquanto(n3 < 201)
	}
}
