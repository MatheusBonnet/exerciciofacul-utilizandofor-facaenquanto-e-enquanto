programa {
	funcao inicio() {
		
		escreva("PARA", "\n")
		para(inteiro n = 1; n < 101; n++){
		    escreva(n, "\n")
		}
		escreva("ENQUANTO","\n")
		inteiro n2 = 1
		enquanto(n2 < 101){
		   escreva(n2, "\n")
		    n2++
		}
		
		escreva("FACA ENQUANTO","\n")
		inteiro n3 = 1
		faca{
		    escreva(n3, "\n")
		    n3++
		}
	    enquanto(n3 < 101)
	}
}
